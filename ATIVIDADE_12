programa
{
    funcao inicio()
    {
        inteiro numero, maior

        
        maior = 0 

        inteiro contador = 1

        enquanto (contador <= 5)
        {
            escreva("Digite o ", contador, "º número: ")
            leia(numero)

            se (numero > maior)
            {
                maior = numero
            }
            contador++
        }

        escreva("O maior número digitado foi: ", maior)
    }
}
