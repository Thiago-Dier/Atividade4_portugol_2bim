programa
{
	
	funcao inicio()
	{
		inteiro base, expoente
		inteiro resultado = 1

		escreva ("Qual a base? ")
		leia (base)
		
		escreva ("Qual o expoente? ")
		leia (expoente)

		para(inteiro i=0; i < expoente; i++){
			resultado = resultado * base
		}

		escreva ("O resultado Ã© ", resultado)
		
	}
}
